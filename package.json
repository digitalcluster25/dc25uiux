{
  "name": "dc25-uiux",
  "version": "1.0.2",
  "description": "Библиотека компонентов shadcn/ui и UI/UX boilerplate для быстрой разработки современных интерфейсов",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,less}\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "generate": "node scripts/cli/generator.js",
    "generate:advanced": "node scripts/cli/advanced-generator.js",
    "changelog": "node scripts/changelog.js",
    "changelog:full": "node scripts/changelog.js --full",
    "changelog:validate": "node scripts/changelog.js --validate",
    "commit": "git-cz",
    "prepare": "husky",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "version": "standard-version",
    "version:patch": "standard-version --release-as patch",
    "version:minor": "standard-version --release-as minor",
    "version:major": "standard-version --release-as major",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:check": "node scripts/release.js check",
    "release:dry": "node scripts/release.js dry-run",
    "version:history": "node scripts/version-manager.js history",
    "version:changes": "node scripts/version-manager.js changes",
    "version:suggest": "node scripts/version-manager.js suggest",
    "version:current": "node scripts/version-manager.js current",
    "generate:component": "node scripts/generate-component.js",
    "generate:atom": "node scripts/generate-component.js --type=atom",
    "generate:molecule": "node scripts/generate-component.js --type=molecule",
    "generate:organism": "node scripts/generate-component.js --type=organism",
    "generate:yo": "yo dc25-uiux"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "shadcn",
    "ui",
    "ux",
    "components",
    "tailwind",
    "react",
    "typescript",
    "boilerplate"
  ],
  "author": "Digital Cluster 25",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalcluster25/dc25uiux.git"
  },
  "homepage": "https://github.com/digitalcluster25/dc25uiux#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-storybook": "^9.1.10",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "playwright": "^1.55.1",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "standard-version": "^9.5.0",
    "storybook": "^9.1.10",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.20",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0",
    "yeoman-generator": "^7.5.1",
    "yo": "^5.1.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "lucide-react": "^0.294.0",
    "openai": "^6.1.0",
    "react": "^18.0.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.64.0",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.9.3",
    "tailwind-merge": "^2.0.0",
    "zod": "^4.1.11"
  }
}
